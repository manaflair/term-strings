  "license": "MIT",
  "bin": {
    "term-strings": "./build/bin/term-strings.js",
    "term-strings-seqdbg": "./build/bin/term-strings-seqdbg.js"
  },
  "scripts": {
    "test": "mocha --require @babel/register $(find sources -name '*.test.js')",
